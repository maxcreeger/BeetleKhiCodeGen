<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<khimodule name="mReactorByBernard">
	<communication>
		<commands>
			<command name="setTemperature">
				<attributes>
					<attribute name="temp" type="double"/>
				</attributes>
				<timeout>10000</timeout>
				<response>
				    <success>OK</success>
				    <failure>NOK</failure>
				</response>
			</command>
		</commands>
		<sensors>
			<sensor name="TEMPERATURE">
				<attributes>
					<attribute name="VALUE" type="int"/>
				</attributes>
			</sensor>
		</sensors>
	</communication>
</khimodule>